    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${fileDirname}",
            "args": [
                "--upstream-endpoint=localhost:9000", 
                "--allowed-source-subnet=0.0.0.0/0", 
                "--allowed-endpoint=localhost:8099",
                "--upstream-insecure",
                "--aws-region=ca-east-01",
                "--vault-address=https://localhost:8200",
                "--vault-token-location=/vault-agent/s3-reverse-proxy.token",
                "--verbose"
            ]
        }
    ]
}